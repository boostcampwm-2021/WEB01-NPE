{
  "name": "npe",
  "scripts": {
    "start": "nodemon --exec ts-node src/App.ts & npx y-websocket-server",
    "build": "tsc --build",
    "test": "jest",
    "start:pm2": "NODE_ENV=test pm2 start src/App.ts --watch & npx y-websocket-server",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/App.ts & npx y-websocket-server",
    "start:test": "cross-env NODE_ENV=test nodemon --exec ts-node src/App.ts & npx y-websocket-server",
    "start:socket": "npx y-websocket-server",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/faker": "^5.5.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.2",
    "@types/jest-when": "^2.7.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "@types/validator": "^13.7.0",
    "class-validator": "^0.13.1",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "jest-sorted": "^1.0.12",
    "jest-when": "^3.4.2",
    "nodemon": "^2.0.14",
    "sql-formatter": "^4.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typeorm-transactional-tests": "^1.1.5",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.7.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.3.2",
    "type-graphql": "^1.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.38",
    "y-websocket": "^1.3.18",
    "yjs": "^13.5.22"
  }
}
